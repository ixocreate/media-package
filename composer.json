{
    "name": "ixocreate/media-package",
    "description": "IXOCREATE Media Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-fileinfo": "*",
        "cocur/slugify": "^4.0",
        "firebase/php-jwt": "^5.0",
        "intervention/image": "^2.4.1",
        "ixocreate/admin-package": "dev-develop",
        "ixocreate/application": "dev-develop",
        "ixocreate/cache-package": "dev-develop",
        "ixocreate/collection": "^0.2",
        "ixocreate/filesystem-package": "dev-develop",
        "symfony/asset": "^5.2"
    },
    "suggest": {
        "ext-pcntl": "*"
    },
    "require-dev": {
        "ext-gd": ">=2.0",
        "ext-imagick": "*",
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/test": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Media\\": "tests/misc/",
            "Ixocreate\\Test\\Media\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
